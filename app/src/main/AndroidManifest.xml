<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techyshed.friendpaste">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.techyshed.volleyk.BackendVolley"
        android:allowBackup="true"
        android:configChanges="orientation|screenSize"
        android:icon="@drawable/anonymous2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Activity.AddActivity" />
        <activity android:name=".Activity.ViewActivity"></activity>
        <activity android:name=".Activity.StringRequest">
            
        </activity>
    </application>

</manifest>